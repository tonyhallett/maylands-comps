{
  "name": "maylandscomps",
  "version": "1.0.0",
  "description": "",
  "source": "src/index.html",
  "targets": {
    "default": {
      "distDir": "./public"
    }
  },
  "scripts": {
    "clean": "rimraf public",
    "deploy-rules": "firebase deploy --only database",
    "build": "npm run clean && parcel build",
    "addIPV4ToEnv": "ts-node scripts/addIPV4ToEnv.ts",
    "start-parcel": "cross-env NODE_ENV=serve parcel",
    "start-parcel-device": "npm run addIPV4ToEnv && cross-env NODE_ENV=device parcel",
    "start": "firebase emulators:exec --only database \"npm run start-parcel\"",
    "start-device": "firebase emulators:exec --only database \"npm run start-parcel-device\"",
    "start-hosting": "npm run clean && npm run addIPV4ToEnv && cross-env NODE_ENV=device parcel build && firebase emulators:start --only database,hosting",
    "test-non-emulator": "jest --ignoreProjects emulator",
    "test-vscode-jest-emulator": "ts-node scripts/vscode-jest-emulators-command-selector.ts",
    "jest-emulator-test": "ts-node scripts/jest-emulator-test.ts",
    "firebased-jest": "ts-node scripts/firebased-jest.ts",
    "check": "tsc --noEmit",
    "prettier-check": "npx prettier . --check",
    "prettier": "npx prettier . --write",
    "prepare": "husky",
    "pre-commit": "npx lint-staged && npm run check"
  },
  "author": "",
  "license": "ISC",
